{
  "version": "1.0.0-*",

  "dependencies": {
    "Itinero": "0.31.9-rc1"
  },

  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD13", "NETFX_CORE", "DOTNET_CORE" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        }
      },
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Diagnostics.Debug": "4.3.0",
        "System.Runtime": "4.3.0"
      }
    },
    "net4.0": {
      "buildOptions": {
        "define": [ "NET40" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        }
      }
    },
    "net4.5": {
      "buildOptions": {
        "define": [ "NET45" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        }
      },
      "frameworkAssemblies": {
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile147": {
      "buildOptions": {
        "define": [ "PORTABLE147", "PCL" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "mscorlib": "2.0.5.0",
        "System": "2.0.5.0",
        "System.Core": "2.0.5.0"
      }
    }
  }
}
